  {
    "extends": [
      "airbnb"
    ],
    "globals": {
      "graphql": false
    },
    "rules": {
      "max-len": 0,
      "no-plusplus": 0,
      "no-underscore-dangle": ["error", {
        "allowAfterThis": true,
        "allowAfterSuper": true
      }],
      "consistent-return": 0,
      "import/no-extraneous-dependencies": 0,
      "import/prefer-default-export": 0,
      "react/forbid-prop-types": 0,
      "react/no-danger": 0,
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "jsx-a11y/anchor-is-valid": [
        "error",
        {
          "components": [],
          "specialLink": [
            "hrefLeft",
            "hrefRight"
          ],
          "aspects": [
            "noHref",
            "invalidHref",
            "preferButton"
          ]
        }
      ]
    }
  }
